<UserControl x:Class="ctrlChart_Type"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
            >
    <Grid>
    <StackPanel HorizontalAlignment="Center" x:Name="spnlChartType" >
      <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
        <Label x:Name="lblSplitGraph" Foreground="#FFD9D9D9" Content=" Split " />
        <Label x:Name="lblStackedGraph" Foreground="#FFD9D9D9" Content=" Stacked " />
        <Label x:Name="lblAdvantageGraph" Foreground="#FFD9D9D9" Content=" Advantage " />
      </StackPanel>
      <Label Grid.Row="0" x:Name="lblTitle" Foreground="#FF818181" Content="Chart Type" Margin="5, 0,5,0" HorizontalAlignment="Center"/>
    </StackPanel>
  </Grid>
</UserControl>
