<UserControl x:Class="ctrlBar_Type"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
            >
    <Grid>
    <StackPanel HorizontalAlignment="Center" x:Name="spnlBarType">
      <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >

        <Label x:Name="lblTeamChartView"  Foreground="#FFD9D9D9" Content=" Team "  />
        <Label x:Name="lblOwnerChartView" Foreground="#FFD9D9D9" Content=" Owner " />
        <Label x:Name="lblSourceChartView" Foreground="#FFD9D9D9" Content=" Source " />
        <Label x:Name="lblValChartView" Foreground="#FFD9D9D9" Content=" Value " />
      </StackPanel>

      <Label Foreground="#FF818181" x:Name="lblTitle" Content="Bar Type" Margin="5, 0,5,0" HorizontalAlignment="Center"/>

    </StackPanel>
  </Grid>
</UserControl>
